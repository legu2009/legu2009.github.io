$fz = 75
$b()
    font-weight: bold
$x(h)
    unit((h/$fz) ,'rem')
vendor(prop, args)
    -webkit-{prop} args
    -moz-{prop} args
    {prop} args
$s(w, h)
    width: $x(w)
    height: $x(h)
$flex()
    display -webkit-box
    display flex
$flex1()
    -webkit-box-flex 1
    flex 1
$hm(h)
    height: $x(h)
    line-height: $x(h)